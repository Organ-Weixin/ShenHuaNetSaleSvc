			</if>
			<if test="Dimensional != null">
				Dimensional = #{Dimensional}, 
			</if>
			<if test="Sequence != null">
				Sequence = #{Sequence}, 
			</if>
			<if test="UserID != null">
				UserID = #{UserID}, 
			</if>
			<if test="ListingPrice != null">
				ListingPrice = #{ListingPrice}, 
			</if>
			<if test="FeatureNo != null">
				FeatureNo = #{FeatureNo}, 
			</if>
			<if test="SessionId != null">
